{
  "name": "acquisitions",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "module",
  "imports": {
    "#src/*": "./src/*",
    "#config/*": "./src/config/*",
    "#controllers/*": "./src/controllers/*",
    "#middleware/*": "./src/middleware/*",
    "#models/*": "./src/models/*",
    "#routes/*": "./src/routes/*",
    "#services/*": "./src/services/*",
    "#utils/*": "./src/utils/*",
    "#validations/*": "./src/validations/*"
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "format": "prettier",
    "format-check": "prettier --check .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "docker:build": "docker build -t acquisitions-app .",
    "docker:build:dev": "docker build --target development -t acquisitions-app:dev .",
    "docker:build:prod": "docker build --target production -t acquisitions-app:prod .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml --env-file .env.development up",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml --env-file .env.development up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "docker:migrate:dev": "docker-compose -f docker-compose.dev.yml --profile migration run --rm migrate",
    "docker:migrate:prod": "docker-compose -f docker-compose.prod.yml --profile migration run --rm migrate",
    "docker:clean": "docker system prune -a --volumes"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@arcjet/inspect": "^1.0.0-beta.12",
    "@arcjet/node": "^1.0.0-beta.12",
    "@neondatabase/serverless": "^1.0.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "winston": "^3.17.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
